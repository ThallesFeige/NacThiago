@model Mercado

@{
    ViewData["Title"] = "Editar";
}

<h1>Editar</h1>

<form asp-action="Editar" asp-controller="Mercado">

    <input asp-for="MercadoId" />

    <partial name="_Form" />

    <input type="submit" value="Salvar" class="btn btn-outline-success" />
    <a asp-action="Index" class="btn btn-outline-dark">Cancelar</a>


</form>

