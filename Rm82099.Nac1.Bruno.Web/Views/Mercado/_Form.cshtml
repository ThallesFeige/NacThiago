@model Mercado


    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Capacidade"></label>
        <input asp-for="Capacidade" class="form-control" />
    </div>

    <div class="form-group">
        <input asp-for="Novo" />
        <label asp-for="Novo"></label>
    </div>

    <div class="form-group">
        <label asp-for="Porte"></label>
        <select asp-for="Porte" class="form-control"
                asp-items="@Html.GetEnumSelectList<Porte>()">
            <option>Selecione</option>
        </select>
    </div>

    <div class="form-group">
        <label asp-for="DataAbertura"></label>
        <input asp-for="DataAbertura" class="form-control" />
    </div>
